

<MyGrid>:
    spacing:30
    canvas:
        Color:
            rgb:1,1,1
        Rectangle:
            size:self.size
            pos: self.pos

    qr_read_label: QrRead
    transaction_number_label: TransactionNumber
    set_qr_label: SetQr
    credit: Credit
    payout_value: PayoutValue
    payout_label: PayoutLabel
    error_message: ErrorMsg
    alert_message: AlertMsg
    warning_message: WarningMsg
    q1: Q1
    q2: Q2
    q3: Q3
    q4: Q4
    q5: Q5
    q6: Q6
    r1: R1
    r2: R2
    r3: R3
    r4: R4
    r5: R5
    r6: R6


    FloatLayout:
        size_hint: 0.45,0.4
        pos_hint: {'x':0.03,'y':0.03}
        canvas:
            Color:
                rgb:0.4,0.4,0.3
            Rectangle:
                size:self.size
                pos: self.pos

        Button:
            size_hint: 0.4,0.1 #2% de la pantalla en 'x' y en 'y' se cancela
            pos_hint: {'x':0.3,'y':0.05} #pos en x al 20% y de y al 20%
            text: "Print Receipt"
            on_press:root.print_receipt()

        Label:
            size_hint:0.55,0.1 #2% de la pantalla en 'x' y en 'y' se cancela
            pos_hint: {'x':0.05,'y':0.5}
            text: "Insert Transaction number: "

        TextInput:
            id:TransactionNumber
            size_hint:0.6,0.1 #2% de la pantalla en 'x' y en 'y' se cancela
            pos_hint: {'x':0.1,'y':0.4}

        Label:
            size_hint:0.55,0.1 #2% de la pantalla en 'x' y en 'y' se cancela
            pos_hint: {'x':0.05,'y':0.3}
            text: "Insert code to convert to QR: "

        TextInput:
            id:SetQr
            size_hint:0.6,0.1 #2% de la pantalla en 'x' y en 'y' se cancela
            pos_hint: {'x':0.1,'y':0.2}


        

    FloatLayout:
        size_hint: 0.45,0.4
        pos_hint: {'x':0.52,'y':0.03}
        canvas:
            Color:
                rgb:0.4,0.4,0.3
            Rectangle:
                size:self.size
                pos: self.pos

        Label:
            size_hint:0.4,None #2% de la pantalla en 'x' y en 'y' se cancela
            height: self.width/4
            pos_hint: {'x':0.05,'y':0.4}
            text: "Text read: "

        TextInput:
            id:QrRead
            size_hint:0.6,None #2% de la pantalla en 'x' y en 'y' se cancela
            height: self.width/4
            pos_hint: {'x':0.1,'y':0.3}



        Button:
            size_hint: 0.4,None #2% de la pantalla en 'x' y en 'y' se cancela
            height: self.width/4
            pos_hint: {'x':0.3,'y':0.2}
            text: "Read Qr Code"
            on_press:root.read_qr()


    
    FloatLayout:
        size_hint: 0.94,0.5
        pos_hint: {'x':0.03,'y':0.45}
        canvas:
            Color:
                rgb:0.4,0.4,0.3
            Rectangle:
                size:self.size
                pos: self.pos

        Label:
            size_hint:0.1,0.1 #2% de la pantalla en 'x' y en 'y' se cancela
            pos_hint: {'x':0.05,'y':0.8}
            text: "Credit: "
        Label:
            id:Credit
            size_hint:0.1,0.1 #2% de la pantalla en 'x' y en 'y' se cancela
            pos_hint: {'x':0.1,'y':0.8}
            text: "$ 0"


        TextInput:
            id:PayoutValue
            size_hint:0.25,0.1 #2% de la pantalla en 'x' y en 'y' se cancela
            pos_hint: {'x':0.05,'y':0.3}
        Button:
            size_hint: 0.2,0.15 #2% de la pantalla en 'x' y en 'y' se cancela
            pos_hint: {'x':0.05,'y':0.1}
            text: "Make Payout"
            on_press:root.make_payout()
        Label:
            id:PayoutLabel
            size_hint: 0.3,0.1
            pos_hint: {'x':0.05, 'y':0.4}
            color:[255, 1, 1, 1]


        Label:
